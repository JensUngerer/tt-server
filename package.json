{
  "name": "tt-server",
  "version": "0.0.1",
  "author": "Jens Ungerer",
  "homepage": "jens-ungerer.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/JensUngerer/tt-server.git"
  },
  "license": "GPL",
  "scripts": {
    "start:ci-windows": "node ./dist/%npm_package_name%",
    "start:ci-linux": "node ./dist/$npm_package_name",
    "start:dev-linux": "node --inspect ./dist/$npm_package_name",
    "start:dev-windows": "node --inspect ./dist/%npm_package_name%",
    "build:dev-linux": "webpack --progress --env development --env linux --stats-error-details true",
    "build:dev": "webpack --env development",
    "start": "webpack --progress --env development --env nodemon",
    "start:prod": "webpack --progress --env nodemon",
    "build:prod": "webpack --progress --stats-error-details true",
    "build:prod-linux": "webpack --progress --env linux --stats-error-details true",
    "build:ci": "webpack",
    "build:docker-linux": "docker build --rm -t toxsickcoder/$npm_package_name:latest .",
    "deploy:docker": "sh ./scripts/deploy.sh",
    "analyse:dev": "webpack --progress --env development --env analyse && start dist/report.html",
    "analyse:prod": "webpack --progress --env analyse && start dist/report.html",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "connect-mongo": "^4.4.0",
    "cors": "^2.8.5",
    "csv": "^5.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "luxon": "^1.26.0",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect-mongo": "^3.1.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.26.2",
    "@types/mongodb": "^3.6.10",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.35",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/passport-local-mongoose": "^4.0.13",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "aws4": "^1.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "nodemon-webpack-plugin": "^4.4.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.27.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
